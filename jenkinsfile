pipeline{
    agent any
    stages{
        stage("clone"){
            steps{
                bat "git clone https://github.com/Kazitanvirazad/jenkins-demo.git"
            }
        }
        stage("compile"){
            steps{
            bat "C:\dev\apache-maven-3.8.4\bin\mvn clean compile"
            }
        }
        
         stage("package"){
            steps{
            bat "C:\dev\apache-maven-3.8.4\bin\mvn package"
            }
        }
       
        
    }
    
   
}
